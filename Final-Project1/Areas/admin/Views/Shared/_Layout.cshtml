    <!-- JQVMap -->
    <script src="/admin/jqvmap/dist/jquery.vmap.js"></script>
    <script src="/admin/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="/admin/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="/admin/moment/min/moment.min.js"></script>
    <script src="/admin/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="/admin/jquery.validate.min.js"></script>
    <script src="/admin/jquery.validate.unobtrusive.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="/admin/custom.min.js"></script>
    <script src="/Admin/content.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
